<?xml version="1.0" encoding="utf-8"?>
<form addrulepath="/administrator/components/com_projects/models/rules">
    <fieldset name="names">
        <field
                name="id"
                type="hidden"
        />
        <field
                name="titleID"
                type="Cattitle"
                label="COM_PROJECTS_BLANK_CATTITLE"
                description="COM_PROJECTS_BLANK_CATTITLE"
                class="x2"
        />
    </fieldset>
    <fieldset name="stand">
        <field
                name="number"
                type="text"
                label="COM_PROJECTS_HEAD_CONTRACT_STAND_NUMBER"
                description="COM_PROJECTS_HEAD_CONTRACT_STAND_NUMBER_DESC"
                hint="COM_PROJECTS_HEAD_CONTRACT_STAND_NUMBER_HINT"
                autocomplete="off"
                class="x2"
        />
        <field
                name="square"
                type="text"
                label="COM_PROJECTS_HEAD_ITEM_HEAD_SQUARE"
                description="COM_PROJECTS_HEAD_ITEM_HEAD_SQUARE_DESC"
                hint="COM_PROJECTS_HEAD_ITEM_HEAD_SQUARE_HINT"
                autocomplete="off"
                class="x2"
        />
    </fieldset>
    <fieldset name="number">
        <field
                name="categoryID"
                type="Hotelnumcat"
                label="COM_PROJECTS_HEAD_NUMBER_CATEGORY"
                description="COM_PROJECTS_HEAD_NUMBER_CATEGORY_DESC"
                class="x2"
        >
        </field>
        <field
                name="title"
                type="text"
                label="COM_PROJECTS_HEAD_NUMBER_TITLE_DESC"
                description="COM_PROJECTS_HEAD_NUMBER_TITLE_DESC"
                class="x2"
        />
    </fieldset>
    <fieldset name="transport">
        <field
            name="gos_number"
            type="text"
            label="COM_PROJECTS_HEAD_NUMBER_GOS_NUMBER"
            description="COM_PROJECTS_HEAD_NUMBER_GOS_NUMBER_DESC"
            hint="COM_PROJECTS_HEAD_NUMBER_GOS_NUMBER_HINT"
            filter="alnum"
            maxlength="10"
            />
    </fieldset>
</form>